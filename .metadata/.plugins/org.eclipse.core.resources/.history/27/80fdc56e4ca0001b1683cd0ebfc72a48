package shiwakoti.ishu;

abstract class Movie {
		
	protected String name;
	protected int price;
	int seat;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}
	
	void byOne() {
		this.seat--;
	}
	abstract void setData();
	

}
