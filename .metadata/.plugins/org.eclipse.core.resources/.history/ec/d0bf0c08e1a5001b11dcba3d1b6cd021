package np.edu.deerwalk;

public class Breakfast extends Food{
	
	public Breakfast() {
	}
	
	Breakfast(int people, int budget){
		super.budget = budget;
		super.people = people;
	}
	
	void checkmenu() {
		if ((people<1)&&(budget<500)){
            System.out.println("");
            System.out.println("");
        }
        else if ((people<2)&&(budget<1000)){
            System.out.println("");

        }else if ((people<=6)&&(budget<1500)){
            System.out.println("");
        }
	}

}
